<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:fo="http://www.w3.org/1999/XSL/Format"><xsl:output method="xml" version="1.0" indent="yes"/><xsl:template match="/">	<fo:root>		<!-- standart measurement -->		<fo:layout-master-set>			<fo:simple-page-master page-height="297mm" page-width="210mm"				margin="5mm 25mm 5mm 25mm" master-name="page">				<fo:region-body margin="20mm 0mm 20mm 0mm"/>			</fo:simple-page-master>		</fo:layout-master-set>    		<fo:page-sequence master-reference="page">			<fo:flow flow-name="xsl-region-body">				<fo:block position ="absolute" text-align ="center" font-weight="bold" margin-left="1cm" margin-right="1cm"				font-family="Arial" font-style="italic" font-size="48pt" padding-before="50mm" color="black" >						<fo:block margin-bottom="30mm">							Каталог на музеите в България						</fo:block>				</fo:block>			</fo:flow>		</fo:page-sequence>				<fo:page-sequence master-reference="page">			<fo:flow flow-name="xsl-region-body">				<fo:block-container position ="relative" top="-2cm" left="-2cm">										<fo:block position="relative" text-align ="center" font-style="italic" font-family="Arial" font-weight="bold" font-size="22pt" color="black" padding-before="5mm">						Музеи в Югозападна България					</fo:block> 					<fo:block position="relative" text-align ="center" font-style="italic" font-family="Arial" font-weight="bold" font-size="15pt" color="black" padding-before="5mm" >						Нациохални музеи					</fo:block> 					<fo:block position="relative" padding-before = "5mm" >									<xsl:for-each select=".//location[@region_id='SW']/museum_type[@m_type_id='National']">						<xsl:apply-templates select="museum"/>					</xsl:for-each>					</fo:block>																				<fo:block position="relative" text-align ="center" font-style="italic" font-family="Arial" font-weight="bold" font-size="22pt" color="black" padding-before="5mm">						Музеи в Северозападна България					</fo:block> 					<fo:block position="relative" text-align ="center" font-style="italic" font-family="Arial" font-weight="bold" font-size="15pt" color="black" padding-before="5mm" >						Регионални музеи					</fo:block> 					<fo:block position="relative" padding-before = "5mm" >									<xsl:for-each select=".//location[@region_id='NW']/museum_type[@m_type_id='Regional_Historical']">						<xsl:apply-templates select="museum"/>					</xsl:for-each>					</fo:block>					<fo:block position="relative" text-align ="center" font-style="italic" font-family="Arial" font-weight="bold" font-size="15pt" color="black" padding-before="5mm" >						Природонаучни музеи					</fo:block> 					<fo:block position="relative" padding-before = "5mm" >									<xsl:for-each select=".//location[@region_id='NW']/museum_type[@m_type_id='Natural_History']">						<xsl:apply-templates select="museum"/>					</xsl:for-each>					</fo:block>															<fo:block position="relative" text-align ="center" font-style="italic" font-family="Arial" font-weight="bold" font-size="22pt" color="black" padding-before="5mm">						Музеи в Североизточна България					</fo:block> 					<fo:block position="relative" text-align ="center" font-style="italic" font-family="Arial" font-weight="bold" font-size="15pt" color="black" padding-before="5mm" >						Археологически музеи					</fo:block> 					<fo:block position="relative" padding-before = "5mm" >									<xsl:for-each select=".//location[@region_id='NE']/museum_type[@m_type_id='Archaeological']">						<xsl:apply-templates select="museum"/>					</xsl:for-each>					</fo:block>															<fo:block position="relative" text-align ="center" font-style="italic" font-family="Arial" font-weight="bold" font-size="22pt" color="black" padding-before="5mm">						Музеи в Северноцентрална България					</fo:block> 					<fo:block position="relative" text-align ="center" font-style="italic" font-family="Arial" font-weight="bold" font-size="15pt" color="black" padding-before="5mm" >						Археологически музеи					</fo:block> 					<fo:block position="relative" padding-before = "5mm" >									<xsl:for-each select=".//location[@region_id='NC']/museum_type[@m_type_id='Etnographic']">						<xsl:apply-templates select="museum"/>					</xsl:for-each>					</fo:block>														</fo:block-container>			</fo:flow>		</fo:page-sequence>	</fo:root></xsl:template><!-- TEMPLATES --><!-- IMAGE --><xsl:template match="image">	<fo:external-graphic src="{unparsed-entity-uri(@href)}" content-height="410" content-width="510"/></xsl:template><!-- prices --><xsl:template match="entry_fee">	<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="10pt" color="black" 		padding-before="5mm">			Пенсионери:			<fo:inline position="relative" padding-left="1mm" font-weight="normal" font-size="10pt" >				<xsl:value-of select="./retiree/text()"/> 			</fo:inline>	</fo:block> 	<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="10pt" color="black" 		padding-before="5mm">			Възрастни:			<fo:inline position="relative" padding-left="1mm" font-weight="normal" font-size="10pt" >				<xsl:value-of select="./adults/text()"/> 			</fo:inline>	</fo:block>	<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="10pt" color="black" 		padding-before="5mm">			Деца:			<fo:inline position="relative" padding-left="1mm" font-weight="normal" font-size="10pt" >				<xsl:value-of select="./children/text()"/> 			</fo:inline>	</fo:block>	<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="10pt" color="black" 		padding-before="5mm">			Студенти:			<fo:inline position="relative" padding-left="1mm" font-weight="normal" font-size="10pt" >				<xsl:value-of select="./students/text()"/> 			</fo:inline>	</fo:block>	<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="10pt" color="black" 		padding-before="5mm">			Инвалиди:			<fo:inline position="relative" padding-left="1mm" font-weight="normal" font-size="10pt" >				<xsl:value-of select="./disabled/text()"/> 			</fo:inline>	</fo:block>	<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="10pt" color="black" 		padding-before="5mm">			Групи:			<fo:inline position="relative" padding-left="1mm" font-weight="normal" font-size="10pt" >				<xsl:value-of select="./groups/text()"/> 			</fo:inline>	</fo:block></xsl:template><!-- lenguage --><xsl:template match="language">	<fo:block position="relative" font-family="Arial" font-size="10pt" color="black" 		padding-before="5mm">			- на			<fo:inline position="relative" padding-left="1mm" font-weight="normal" font-size="10pt" >				<xsl:value-of select="./text()"/> 			</fo:inline>	</fo:block></xsl:template><!-- contacts/phone --><xsl:template match="phone">	<fo:block position="relative" font-family="Arial" font-size="10pt" color="black" 		padding-before="5mm">			тел.			<fo:inline position="relative" padding-left="1mm" font-weight="normal" font-size="10pt" >				<xsl:value-of select="./text()"/> 			</fo:inline>	</fo:block></xsl:template><!-- contacts/email --><xsl:template match="email">	<fo:block position="relative" font-family="Arial" font-size="10pt" color="black" 		padding-before="5mm">			email:			<fo:inline position="relative" padding-left="1mm" font-weight="normal" font-size="10pt" >				<xsl:value-of select="./text()"/> 			</fo:inline>	</fo:block></xsl:template><!-- working hours --><xsl:template match="working_hours">	<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="10pt" color="black" 		padding-before="5mm">			Отворено:			<fo:inline position="relative" padding-left="1mm" font-weight="normal" font-size="10pt" >				<xsl:value-of select="./working_days/text()"/> 			</fo:inline>	</fo:block> 	<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="10pt" color="black" 		padding-before="5mm">			Затворено:			<fo:inline position="relative" padding-left="1mm" font-weight="normal" font-size="10pt" >				<xsl:value-of select="./non-working_days/text()"/> 			</fo:inline>	</fo:block> </xsl:template><!-- mususem --><xsl:template match="museum">	<fo:block position="relative" text-align="center" font-family="Arial" font-style="italic" font-weight="bold" color="black" font-size="19pt"		padding-before="5mm">				<xsl:value-of select="./name/text()"/> 		</fo:block> 				<fo:block position="absolute" text-align="center" padding-before="10mm" padding-after="5mm" >			<xsl:apply-templates select="./image"/>		</fo:block>				<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="14pt" color="black" 		padding-before="5mm">			Описание:			<fo:inline position="relative" padding-left="5mm" font-weight="normal" font-size="14pt" >				<xsl:value-of select="./description/text()"/> 			</fo:inline>		</fo:block> 				<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="14pt" color="black" 		padding-before="5mm">			Адрес:			<fo:inline position="relative" padding-left="5mm" font-weight="normal" font-size="14pt" >				<xsl:value-of select="./address/text()"/> 			</fo:inline>		</fo:block> 				<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="14pt" color="black" 		padding-before="5mm">		ЦЕНИ:		</fo:block>		<fo:block>			<xsl:apply-templates select="./prices/entry_fee"></xsl:apply-templates>		</fo:block>				<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="14pt" color="black" 		padding-before="5mm">		ЦЕНИ ЗА БЕСЕДИ:		</fo:block>		<fo:block position="relative" font-family="Arial" font-size="14pt" color="black" 		padding-before="1mm">				<xsl:apply-templates select=".//language"/>		</fo:block>				<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="14pt" color="black" 		padding-before="5mm">		Контакти:		</fo:block>		<fo:block position="relative" font-family="Arial" font-size="14pt" color="black" 		padding-before="1mm">				<xsl:apply-templates select=".//phone"/>		</fo:block>		<fo:block position="relative" font-family="Arial" font-size="14pt" color="black" 		padding-before="1mm">				<xsl:apply-templates select=".//email"/>		</fo:block>				<fo:block position="relative" font-family="Arial" font-weight="bold" font-size="14pt" color="black" 		padding-before="5mm">		Работно време:		</fo:block>		<fo:block>			<xsl:apply-templates select="./working_hours"></xsl:apply-templates>		</fo:block>		</xsl:template><!-- contacts --> </xsl:stylesheet>